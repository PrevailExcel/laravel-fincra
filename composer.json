{
    "name": "prevailexcel/laravel-fincra",
    "description": "A Laravel Package for working with Fincra API seamlessly",
    "keywords": [
        "laravel",
        "fincra",
        "payment",
        "checkout",
        "virtual-accounts",
        "payouts",
        "paystack alternative",
        "Fiat Payments",
        "fincra api",
        "payment gateway",
        "online payments",
        "digital currency",
        "digital payments",
        "financial technology",
        "crypto integration",
        "payment solutions",
        "php",
        "github",
        "laravel",
        "Open Source",
        "prevailexcel"
    ],
    "homepage": "https://github.com/prevailexcel/laravel-fincra",
    "license": "MIT",
    "authors": [
        {
            "name": "Chimeremeze Prevail Ejimadu",
            "email": "prevailexcellent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "PrevailExcel\\Fincra\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PrevailExcel\\Fincra\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PrevailExcel\\Fincra\\FincraServiceProvider"
            ],
            "aliases": {
                "Fincra": "PrevailExcel\\Fincra\\Facades\\Fincra"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}